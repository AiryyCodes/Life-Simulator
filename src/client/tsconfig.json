{
	"extends": "../../tsconfig.base.json",
	"compilerOptions": {
		"ignoreDeprecations": "6.0",
		"types": [
			"../../node_modules/@ragempcommunity/types-client",
			"./@types"
		],
		"baseUrl": "./",
		"paths": {
			"@shared/*": [
				"../shared/*"
			],
			"@/*": [
				"./*"
			]
		}
	},
	"include": [
		"./**/*.ts",
		"../shared/**/*.ts",
	],
	"exclude": [
		"../shared/**/*.d.ts"
	]
}