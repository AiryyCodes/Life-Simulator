generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Player {
  id       Int    @id @default(autoincrement())
  username String @unique
  email    String @unique
  password String
}

model Vehicle {
  id        Int    @id @default(autoincrement())
  modelHash BigInt @unique
  modelName String @unique
  name      String
  class     String
}
